{
  "name": "@neo-vue/neo-ui",
  "version": "1.0.0",
  "description": "VueJS UI components library with neobrutalism style design.",
  "main": "lib/index.js",
  "type": "module",
  "scripts": {
    "test-server": "cd test-server && npm run dev",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run format && npm run lint && rollup -c",
